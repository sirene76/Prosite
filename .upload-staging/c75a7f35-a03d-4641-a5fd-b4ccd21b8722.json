  "renderedHtml": "<!DOCTYPE html>\r\n<html lang=\"en\">\r\n<head>\r\n  <meta charset=\"UTF-8\" />\r\n  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\" />\r\n  <title>Strategic Studio</title>\r\n  <link href=\"https://fonts.googleapis.com/css2?family=Montserrat:wght@400;500;600;700&family=Playfair+Display:wght@400;500;600&display=swap\" rel=\"stylesheet\">\r\n  <link rel=\"stylesheet\" href=\"https://sj516b8jo4.ufs.sh/f/pFV1pg27GobSeyyBggH8We0B4QDG76dVYtXrNwnE8cHFLkZf\" />\r\n</head>\r\n<body>\r\n  <div class=\"container\">\r\n    <header>\r\n      <h1>Strategic creative.</h1>\r\n      <p class=\"tagline\">Design + copywriting services to grow your brand.</p>\r\n      <p class=\"services\">Print + Digital. Branding. Packaging. Editorial. Websites. Campaigns. Swag, too.</p>\r\n    </header>\r\n\r\n    <hr />\r\n\r\n    <section id=\"work\" data-module-id=\"work\">\r\n      <h2>The Work.</h2>\r\n      <p class=\"section-intro\">One great thing about creative is that the work speaks for itself.</p>\r\n\r\n      <div class=\"work-grid\">\r\n        <div class=\"work-item\">\r\n          <h3>Graphic Design</h3>\r\n          <a href=\"#\" class=\"work-link\">See more →</a>\r\n        </div>\r\n\r\n        <div class=\"work-item\">\r\n          <h3>Art + Creative Direction</h3>\r\n          <a href=\"#\" class=\"work-link\">Portfolio →</a>\r\n        </div>\r\n\r\n        <div class=\"work-item\">\r\n          <h3>Copywriting</h3>\r\n          <a href=\"#\" class=\"work-link\">Read more →</a>\r\n        </div>\r\n      </div>\r\n    </section>\r\n\r\n    <hr />\r\n\r\n    <section id=\"method\" data-module-id=\"method\">\r\n      <h2>The Method.</h2>\r\n      <p class=\"method-intro\">Bespoke design + copywriting for a select global clientele.</p>\r\n\r\n      <div class=\"method-grid\">\r\n        <div class=\"method-item\">\r\n          <h3>Strategic</h3>\r\n          <p>Less talk, more action.</p>\r\n        </div>\r\n\r\n        <div class=\"method-item\">\r\n          <h3>Design-centric</h3>\r\n          <p>Mindset.</p>\r\n        </div>\r\n\r\n        <div class=\"method-item\">\r\n          <h3>Location independent</h3>\r\n          <p>Global perspective.</p>\r\n        </div>\r\n      </div>\r\n    </section>\r\n\r\n    <footer>\r\n      <p>© Strategic Studio. All rights reserved.</p>\r\n    </footer>\r\n  </div>\r\n</body>\r\n</html>\r\n",
  "css": ":root {\r\n  --primary: #1a1a1a;\r\n  --secondary: #666;\r\n  --accent: #d4af37;\r\n  --light: #f8f8f8;\r\n  --spacing-sm: 0.5rem;\r\n  --spacing-md: 1rem;\r\n  --spacing-lg: 2rem;\r\n  --spacing-xl: 4rem;\r\n}\r\n\r\nbody {\r\n  font-family: 'Montserrat', sans-serif;\r\n  color: var(--primary);\r\n  line-height: 1.6;\r\n  background-color: var(--light);\r\n  margin: 0;\r\n  padding: 0;\r\n}\r\n\r\n.container {\r\n  max-width: 1200px;\r\n  margin: 0 auto;\r\n  padding: 0 var(--spacing-md);\r\n}\r\n\r\n/* Header */\r\nheader {\r\n  padding: var(--spacing-xl) 0;\r\n  text-align: center;\r\n  animation: fadeIn 1.2s ease;\r\n}\r\n\r\nh1 {\r\n  font-family: 'Playfair Display', serif;\r\n  font-size: 3.5rem;\r\n  font-weight: 600;\r\n  letter-spacing: -0.02em;\r\n  margin-bottom: var(--spacing-sm);\r\n}\r\n\r\n.tagline {\r\n  font-size: 1.25rem;\r\n  color: var(--secondary);\r\n  margin-bottom: var(--spacing-lg);\r\n  font-weight: 500;\r\n}\r\n\r\n.services {\r\n  font-size: 1.1rem;\r\n  color: var(--secondary);\r\n  max-width: 600px;\r\n  margin: 0 auto;\r\n  line-height: 1.8;\r\n}\r\n\r\nhr {\r\n  border: none;\r\n  height: 1px;\r\n  background-color: #ddd;\r\n  margin: var(--spacing-xl) 0;\r\n}\r\n\r\n/* Sections */\r\nsection {\r\n  margin-bottom: var(--spacing-xl);\r\n}\r\n\r\nh2 {\r\n  font-family: 'Playfair Display', serif;\r\n  font-size: 2.5rem;\r\n  font-weight: 500;\r\n  margin-bottom: var(--spacing-lg);\r\n  text-align: center;\r\n}\r\n\r\n.section-intro {\r\n  text-align: center;\r\n  max-width: 700px;\r\n  margin: 0 auto var(--spacing-lg);\r\n  color: var(--secondary);\r\n  font-size: 1.1rem;\r\n}\r\n\r\n/* Work */\r\n.work-grid {\r\n  display: grid;\r\n  grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));\r\n  gap: var(--spacing-lg);\r\n}\r\n\r\n.work-item {\r\n  text-align: center;\r\n  padding: var(--spacing-lg);\r\n  border-radius: 8px;\r\n  transition: transform 0.3s ease, box-shadow 0.3s ease;\r\n  background: rgba(255, 255, 255, 0.6);\r\n}\r\n\r\n.work-item:hover {\r\n  transform: translateY(-5px);\r\n  box-shadow: 0 10px 20px rgba(0, 0, 0, 0.05);\r\n}\r\n\r\n.work-item h3 {\r\n  font-size: 1.5rem;\r\n  font-weight: 600;\r\n  margin-bottom: var(--spacing-md);\r\n}\r\n\r\n.work-link {\r\n  display: inline-block;\r\n  margin-top: var(--spacing-md);\r\n  color: var(--accent);\r\n  text-decoration: none;\r\n  font-weight: 500;\r\n  position: relative;\r\n}\r\n\r\n.work-link::after {\r\n  content: '';\r\n  position: absolute;\r\n  width: 0;\r\n  height: 1px;\r\n  bottom: -2px;\r\n  left: 0;\r\n  background-color: var(--accent);\r\n  transition: width 0.3s ease;\r\n}\r\n\r\n.work-link:hover::after {\r\n  width: 100%;\r\n}\r\n\r\n/* Method */\r\n.method-intro {\r\n  text-align: center;\r\n  max-width: 600px;\r\n  margin: 0 auto var(--spacing-xl);\r\n  font-size: 1.1rem;\r\n  color: var(--secondary);\r\n}\r\n\r\n.method-grid {\r\n  display: grid;\r\n  grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));\r\n  gap: var(--spacing-md);\r\n}\r\n\r\n.method-item {\r\n  padding: var(--spacing-lg);\r\n  text-align: center;\r\n  border-left: 1px solid #eee;\r\n  background: rgba(255, 255, 255, 0.6);\r\n  transition: background 0.3s ease;\r\n}\r\n\r\n.method-item:hover {\r\n  background: rgba(255, 255, 255, 0.9);\r\n}\r\n\r\n.method-item:first-child {\r\n  border-left: none;\r\n}\r\n\r\n.method-item h3 {\r\n  font-size: 1.3rem;\r\n  font-weight: 600;\r\n  margin-bottom: var(--spacing-sm);\r\n}\r\n\r\n.method-item p {\r\n  color: var(--secondary);\r\n}\r\n\r\n/* Footer */\r\nfooter {\r\n  text-align: center;\r\n  padding: var(--spacing-xl) 0;\r\n  color: var(--secondary);\r\n  border-top: 1px solid #eee;\r\n  margin-top: var(--spacing-xl);\r\n}\r\n\r\n/* Animations */\r\n@keyframes fadeIn {\r\n  from { opacity: 0; transform: translateY(20px); }\r\n  to { opacity: 1; transform: translateY(0); }\r\n}\r\n\r\n/* Responsive */\r\n@media (max-width: 768px) {\r\n  h1 {\r\n    font-size: 2.5rem;\r\n  }\r\n\r\n  h2 {\r\n    font-size: 2rem;\r\n  }\r\n\r\n  .method-item {\r\n    border-left: none;\r\n    border-top: 1px solid #eee;\r\n    padding: var(--spacing-md) 0;\r\n  }\r\n\r\n  .method-item:first-child {\r\n    border-top: none;\r\n  }\r\n}\r\n",
